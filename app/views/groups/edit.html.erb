<div class="col-md-4 col-md-offset-4">
  <h2> Edit bulletin board <h2>

  <hr>

  <%= form_for @group do |f| %>

  Bulletin Title
  <br>
  <%= f.text_field :title %>
  <br>
  Description
  <br>
  <%= f.text_area :description %>
  <br>

  <%= f.submit "Submit", :disable_with => "Submitting..." %>
  <% end %>


</div>
